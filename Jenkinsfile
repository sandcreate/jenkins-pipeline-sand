pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello from github hook trigger World'
            }
        }
        stage('Buld') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Relase') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
